<!doctype html>
<html lang="en">

<head>
  <!-- Required meta tags -->
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">

  <!-- Bootstrap CSS -->
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.1/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-iYQeCzEYFbKjA/T2uDLTpkwGzCiq6soy8tYaI1GyVh/UjpbCx/TYkiZhlZB6+fzT" crossorigin="anonymous">
  <!-- Bootstrap JavaScript -->
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.1/dist/js/bootstrap.bundle.min.js" 
  integrity="sha384-u1OknCvxWvY5kfmNBILK2hRnQC3Pr17a+RTT6rIHI7NnikvbZlHgTPOOmMi466C8" 
  crossorigin="anonymous"></script>
  <!--Plotly JS-->
  <script src="https://cdn.plot.ly/plotly-2.14.0.min.js"></script>
  <title>{{ jenkins_pipeline_name }} Pipeline Status</title>
</head>

<body>
  <div class="container px-2 mt-5">
    <a href="{{jenkins_pipeline_url}}"><img class="d-block mx-auto mb-4" src="assets/{{ logo }}" alt="Logo" height="110"></a>
    <h1 class="display-3 fw-bold text-center">Pipeline Status</h1>
    <div class="col-lg-12 mx-auto">
      <h2 class="display-5 text-center">{{ jenkins_pipeline_name }}</h2>
      <p class="text-muted text-center">(Updated on {{ current_datetime }})</p>
      <div class="col-lg-9 mx-auto text-center">
        <h3>Overall</h3>       
      </div>
      <div class="col-lg-9 mx-auto">
        {{overall_chart_html_code}}        
      </div>
      <div class="col-lg-9 mx-auto text-center">
        <div class="card mb-4 rounded-3 shadow-sm">
          <div class="card-header py-2">
            <h4 class="my-0 fw-normal">Test Result</h4>
          </div>
          <table class="table table-sm mb-0">
            <thead>
              <tr>
                <th scope="col"></th>
                {% for num in aggregate_test_data[overall_key][aggregate_columns_key[0]] %}
                <th scope="col">#{{ num }}</th>
                {% endfor %}
              </tr>
            </thead>
            <tbody>
              {% for outcome in aggregate_columns_key[1:] %}
              <tr class="table-{{color_scheme[outcome]}}">
                <th scope="row">{{outcome}}</th>
                {% for num in aggregate_test_data[overall_key][outcome] %}
                <td>{{ num }}</td>
                {% endfor %}
              </tr>
              {% endfor %}
            </tbody>
          </table>
        </div>
      </div>
      <div class="col-lg-9 mx-auto text-center">
        <h3>By Build and Environment</h3>       
      </div>
      <ul class="nav nav-tabs nav-fill mb-3" role="tablist" id="os-tabs">
        {% for os_key in os_keys %}
        <li class="nav-item">
          <a 
            class="nav-link" 
            data-bs-toggle="tab" 
            href="#{{os_key}}"
            >
            <h3>{{os_key}}</h3>
          </a>
        </li>
        {% endfor %}
      </ul>
      <div class="tab-content" id="os-tabs-content">
        {% for os_key in os_keys %}
        <div
          class="container tab-pane fade"
          id="{{os_key}}"
          role="tabpanel"
          aria-labelledby="ex2-tab-1"
        >
          <div class="col-lg-9 mx-auto">
            {{by_os_chart_html_code[os_key]}}        
          </div>
          <div class="col-lg-9 mx-auto text-center">
            <div class="card mb-4 rounded-3 shadow-sm">
              <div class="card-header py-2">
                <h4 class="my-0 fw-normal">Test Result</h4>
              </div>
              <table class="table table-sm mb-0">
                <thead>
                  <tr>
                    <th scope="col"></th>
                    {% for num in aggregate_test_data[os_key][aggregate_columns_key[0]] %}
                    <th scope="col">#{{ num }}</th>
                    {% endfor %}
                  </tr>
                </thead>
                <tbody>
                  {% for outcome in aggregate_columns_key[1:] %}
                  <tr class="table-{{color_scheme[outcome]}}">
                    <th scope="row">{{outcome}}</th>
                    {% for num in aggregate_test_data[overall_key][outcome] %}
                    <td>{{ num }}</td>
                    {% endfor %}
                  </tr>
                  {% endfor %}
                </tbody>
              </table>
            </div>
          </div>
          <div class="col-lg-9 mx-auto text-center">
            <h3>Stack Trace</h3>       
          </div>
          <ul class="nav nav-tabs nav-fill mb-3" role="tablist" id="os-tabs">
            {% for build in builds_key %}
            <li class="nav-item">
              <a 
                class="nav-link" 
                data-bs-toggle="tab" 
                href="#{{os_key}}-{{build}}-stack-trace"
                >
                <h4>{{build}}</h4>
              </a>
            </li>
            {% endfor %}
          </ul>
          <div class="tab-content" id="{{os_key}}-stack-trace-tabs-content">
            {% for build in builds_key %}
            <div
              class="container tab-pane fade"
              id="{{os_key}}-{{build}}-stack-trace"
              role="tabpanel"
              aria-labelledby="ex2-tab-1"
            >
              <div id="{{os_key}}-{{build}}-accordion">
                {% for outcome in aggregate_test_data[os_key][aggregate_columns_key[3:]] %}
                <div class="card bg-{{color_scheme[outcome]}} text-white">
                  <div class="card-header">
                    <a class="d-block text-white" data-bs-toggle="collapse" href="#{{os_key}}-{{build}}-{{outcome}}-collapse">
                      <h5>{{outcome}}</h5>
                    </a>
                  </div>
                  <div id="{{os_key}}-{{build}}-{{outcome}}-collapse" class="collapse" data-bs-parent="#{{os_key}}-{{build}}-accordion">
                    <div class="card-body">
                      <div id="{{os_key}}-{{build}}-{{outcome}}-accordion">
                        {% for test in stack_trace[os_key][build][outcome] %}
                        <div class="card bg-{{color_scheme[outcome]}} text-white">
                          <div class="card-header">
                            <a class="d-block text-white" data-bs-toggle="collapse" href="#{{os_key}}-{{build}}-{{outcome}}-{{test[0]}}-collapse">
                              <h5>Test: #{{test[0]}} - Name: {{test[1]}}</h5>
                            </a>
                          </div>
                          <div id="{{os_key}}-{{build}}-{{outcome}}-{{test[0]}}-collapse" class="collapse" data-bs-parent="#{{os_key}}-{{build}}-{{outcome}}-accordion">
                            <div class="card-body">
                              <div id="{{os_key}}-{{build}}-{{outcome}}-{{test[0]}}-accordion">
                                {% for trial in test[2] %}
                                <div class="card bg-{{color_scheme[trial[1]]}} text-white text-left">
                                  <div class="card-header">
                                    <a class="d-block text-white bg-{{color_scheme[trial[1]]}}" data-bs-toggle="collapse" href="#{{os_key}}-{{build}}-{{outcome}}-{{test[0]}}-{{trial[0]}}-collapse">
                                      <h5>Trial: {{trial[0]}} - Outcome: {{trial[1]}} - Test Time: {{trial[2]}} sec</h5>
                                    </a>
                                  </div>
                                  <div id="{{os_key}}-{{build}}-{{outcome}}-{{test[0]}}-{{trial[0]}}-collapse" class="collapse" data-bs-parent="#{{os_key}}-{{build}}-{{outcome}}-{{test[0]}}-accordion">
                                    <div class="card-body bg-info">
                                      <pre><code>{{trial[3].replace("\n", "<br>")}}</code></pre>
                                    </div>
                                  </div>
                                </div>
                                {% endfor %}
                              </div>
                            </div>
                          </div>
                        </div>
                        {% endfor %}
                      </div>
                    </div>
                  </div>
                </div>
                {% endfor %}

              </div>
            </div>
            {% endfor %}
          </div>
        </div>
        {% endfor %}
      </div>
    </div>
  </div>
</body>

</html>