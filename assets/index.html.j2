<!doctype html>
<html lang="en">

<head>
  <!-- Required meta tags -->
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">

  <!-- Bootstrap CSS (Uncomment this foe CDN)
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.1/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-iYQeCzEYFbKjA/T2uDLTpkwGzCiq6soy8tYaI1GyVh/UjpbCx/TYkiZhlZB6+fzT" crossorigin="anonymous">
  -->
  <!-- Bootstrap JavaScript (Uncomment this foe CDN)
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.1/dist/js/bootstrap.bundle.min.js" 
  integrity="sha384-u1OknCvxWvY5kfmNBILK2hRnQC3Pr17a+RTT6rIHI7NnikvbZlHgTPOOmMi466C8" 
  crossorigin="anonymous"></script>
  -->
  <!--Plotly JS (Uncomment this foe CDN)
  <script src="https://cdn.plot.ly/plotly-2.14.0.min.js"></script>
  -->
  <!--Local CSS install-->
  <link rel="stylesheet" href="assets/src/{{bootstrap_css_file}}">
  <script src="assets/src/{{bootstrap_js_file}}"></script>
  <script src="assets/src/{{plotly_js_file}}"></script>

  <title>{{ jenkins_pipeline_name }} Pipeline Status</title>


</head>

<body>
  <!--Page Container-->
  <div class="container px-2 mt-5">
    <!--Logo-->
    <a href="{{jenkins_pipeline_url}}"><img class="d-block mx-auto mb-4" src="assets/{{ logo }}" alt="Logo" height="110"></a>
    <!--Page Container Heading-->
    <h1 class="display-3 fw-bold text-center">Pipeline Status</h1>
    <!--Sub Heading-->
    <div class="col-lg-12 mx-auto">
      <h2 class="display-5 text-center">{{ jenkins_pipeline_name }}</h2>
      <p class="text-muted text-center">(Updated on {{ current_datetime }})</p>
      <div class="col-lg-9 mx-auto text-center">
        <h3>Overall</h3>       
      </div>
      <!--Overall Chart by Plotly-->
      <div class="col-lg-9 mx-auto">
        {{overall_chart_html_code}}        
      </div>
      <!--Overall aggregate result table-->
      <div class="col-lg-9 mx-auto text-center">
        <div class="card mb-4 rounded-3 shadow-sm">
          <div class="card-header py-2">
            <h4 class="my-0 fw-normal">Test Result</h4>
          </div>
          <table class="table table-sm mb-0">
            <thead>
              <!--Table Column-->
              <tr>
                <th scope="col"></th>
                {% for num in aggregate_test_data[overall_key][aggregate_columns_key[0]] %}
                <th scope="col">#{{ num }}</th>
                {% endfor %}
              </tr>
            </thead>
            <!--Table body row-->
            <tbody>
              {% for outcome in aggregate_columns_key[1:] %}
              <tr class="table-{{data_name_style[outcome].bootstrap_style}}">
                <th scope="row">{{outcome}}</th>
                {% for num in aggregate_test_data[overall_key][outcome] %}
                <td>{{ num }}</td>
                {% endfor %}
              </tr>
              {% endfor %}
            </tbody>
          </table>
        </div>
      </div>
      <!--Result by environment part-->
      <div class="col-lg-9 mx-auto text-center">
        <h3>By Build and Environment</h3>       
      </div>
      <!--Result by environment tabs heading loop-->
      <ul class="nav nav-tabs nav-fill mb-3" role="tablist" id="os-tabs">
        {% for os_key in os_keys %}
        <li class="nav-item">
          <a 
            class="nav-link" 
            data-bs-toggle="tab" 
            href="#{{os_key}}"
            >
            <h3>{{os_key}}</h3>
          </a>
        </li>
        {% endfor %}
      </ul>
      <!--Result by environment tabs content loop-->
      <div class="tab-content" id="os-tabs-content">
        {% for os_key in os_keys %}
        <div
          class="container tab-pane fade"
          id="{{os_key}}"
          role="tabpanel"
          aria-labelledby="ex2-tab-1"
        >
          <!--Result by environment Chart-->
          <div class="col-lg-9 mx-auto">
            {{by_os_chart_html_code[os_key]}}        
          </div>
          <!--Result by environment Table-->
          <div class="col-lg-9 mx-auto text-center">
            <div class="card mb-4 rounded-3 shadow-sm">
              <div class="card-header py-2">
                <h4 class="my-0 fw-normal">Test Result</h4>
              </div>
              <table class="table table-sm mb-0">
                <thead>
                  <tr>
                    <th scope="col"></th>
                    {% for num in aggregate_test_data[os_key][aggregate_columns_key[0]] %}
                    <th scope="col">#{{ num }}</th>
                    {% endfor %}
                  </tr>
                </thead>
                <tbody>
                  {% for outcome in aggregate_columns_key[1:] %}
                  <tr class="table-{{data_name_style[outcome].bootstrap_style}}">
                    <th scope="row">{{outcome}}</th>
                    {% for num in aggregate_test_data[overall_key][outcome] %}
                    <td>{{ num }}</td>
                    {% endfor %}
                  </tr>
                  {% endfor %}
                </tbody>
              </table>
            </div>
          </div>
          <!--Stack trace-->
          <div class="col-lg-9 mx-auto text-center">
            <h3>Stack Trace</h3>       
          </div>
          <!--Stack trace tab loop-->
          <ul class="nav nav-tabs nav-fill mb-3" role="tablist" id="os-tabs">
            {% for build in build_keys %}
            <li class="nav-item">
              <a 
                class="nav-link" 
                data-bs-toggle="tab" 
                href="#{{os_key}}-{{build}}-stack-trace"
                >
                <h4>#{{build}}</h4>
              </a>
            </li>
            {% endfor %}
          </ul>
          <!--Stack trace tab loop by build-->
          <div class="tab-content" id="{{os_key}}-stack-trace-tabs-content">
            {% for build in build_keys %}
            <div
              class="container tab-pane fade"
              id="{{os_key}}-{{build}}-stack-trace"
              role="tabpanel"
              aria-labelledby="ex2-tab-1"
            >
              <!--Stack trace tab loop sorted by result type-->
              <div id="{{os_key}}-{{build}}-accordion">
                {% for outcome in stack_trace_columns %}
                <div class="card bg-{{data_name_style[outcome].bootstrap_style}} text-white">
                  <div class="card-header">
                    <a class="d-block text-white" data-bs-toggle="collapse" href="#{{os_key}}-{{build}}-{{outcome}}-collapse">
                      <h5>{{outcome}}</h5>
                    </a>
                  </div>
                  <div id="{{os_key}}-{{build}}-{{outcome}}-collapse" class="collapse" data-bs-parent="#{{os_key}}-{{build}}-accordion">
                    <div class="card-body">
                      <!--Stack trace tab loop sorted by test number the test is groupped into a nested list for easier looping
                      refer to grok_parser.py for how it is formated
                      -->
                      <div id="{{os_key}}-{{build}}-{{outcome}}-accordion">
                        {% for test in stack_trace[os_key][build][outcome] %}
                        <div class="card bg-{{data_name_style[outcome].bootstrap_style}} text-white">
                          <div class="card-header">
                            <a class="d-block text-white text-decoration-none" data-bs-toggle="collapse" href="#{{os_key}}-{{build}}-{{outcome}}-{{test.test_name}}-collapse">
                              <h5>Test Name: {{test.test_name}}</h5>
                            </a>
                          </div>
                          <!--Stack trace tab loop sorted by trial-->
                          <div id="{{os_key}}-{{build}}-{{outcome}}-{{test.test_name}}-collapse" class="collapse" data-bs-parent="#{{os_key}}-{{build}}-{{outcome}}-accordion">
                            <div class="card-body">
                              <div id="{{os_key}}-{{build}}-{{outcome}}-{{test.test_name}}-accordion">
                                {% for trial in test.trials %}
                                {{trial}}
                                <div class="card bg-{{data_name_style[trial.outcome].bootstrap_style}} text-white text-left">
                                  <div class="card-header">
                                    <a class="d-block text-white bg-{{data_name_style[trial.outcome].bootstrap_style}}" data-bs-toggle="collapse" href="#{{os_key}}-{{build}}-{{outcome}}-{{test.test_name}}-{{trial.trial_num}}-collapse">
                                      <h5>Trial: {{trial.trial_num}} - Outcome: {{trial.outcome}} - Test Time: {{trial.test_time}} sec</h5>
                                    </a>
                                  </div>
                                  <div id="{{os_key}}-{{build}}-{{outcome}}-{{test.test_name}}-{{trial.trial_num}}-collapse" class="collapse" data-bs-parent="#{{os_key}}-{{build}}-{{outcome}}-{{test.test_name}}-accordion">
                                    <div class="card-body bg-dark">
                                      <pre><code>{{trial.stack_trace}}</code></pre>
                                    </div>
                                  </div>
                                </div>
                                {% endfor %}
                              </div>
                            </div>
                          </div>
                          <!--Stack trace tab loop sorted by trial (end)-->
                        </div>
                        {% endfor %}
                      </div>
                      <!--Stack trace tab loop sorted by test number (end)-->
                    </div>
                  </div>
                </div>
                {% endfor %}
              </div>
              <!--Stack trace tab loop sorted by result type (end)-->
            </div>
            {% endfor %}
          </div>
        </div>
        {% endfor %}
      </div>
    </div>
  </div>
</body>

</html>